	$('.periodoFormacao').mask('0000 - 0000', {
		placeholder : "____ - ____"
	});
	$('.periodoExperiencia').mask('00/00/0000 até 00/00/0000', {
		placeholder : "__/__/___ até __/__/___"
	});
	$('.periodoCurso').mask('00/0000 até 00/0000', {
		placeholder : "__/___ até __/___"
	});
	$('.periodoInicio').mask('00/0000', {
		placeholder : "__/___"
	});
	$('.cpfLogin').mask('000.000.000-00', {
		placeholder : "___.___.___-__"
	});
});